{
  "name": "{{name}}",
  "version": "1.0.0",
  "description": "{{description}}",
  "private": true,
  "egg": {
    "typescript": true
  },
  "scripts": {
    "start": "eggctl start --daemon --title=egg-server-{{name}}",
    "stop": "eggctl stop --title=egg-server-{{name}}",
    "dev": "egg-bin dev",
    "test:local": "egg-bin test",
    "pretest": "npm run clean && npm run lint -- --fix",
    "test": "egg-bin test",
    "preci": "npm run clean && npm run lint",
    "ci": "egg-bin cov",
    "postci": "npm run prepublishOnly && npm start && npm stop && npm run clean",
    "lint": "eslint . --ext .ts --cache",
    "tsc": "tsc",
    "clean": "tsc -b --clean",
    "prepublishOnly": "npm run clean && npm run tsc"
  },
  "dependencies": {
    "@eggjs/tegg": "^3.5.2",
    "@eggjs/tegg-aop-plugin": "^3.5.2",
    "@eggjs/tegg-config": "^3.5.2",
    "@eggjs/tegg-controller-plugin": "^3.5.2",
    "@eggjs/tegg-eventbus-plugin": "^3.5.2",
    "@eggjs/tegg-plugin": "^3.5.2",
    "@eggjs/tegg-schedule-plugin": "^3.5.2",
    "egg": "^4.0.0",
    "@eggjs/scripts": "^4.0.0",
    "egg-tracer": "^2.0.0"
  },
  "devDependencies": {
    "@types/mocha": "10",
    "@types/node": "22",
    "@eggjs/tsconfig": "1",
    "@eggjs/bin": "7",
    "@eggjs/mock": "6",
    "eslint": "8",
    "eslint-config-egg": "14",
    "typescript": "5"
  },
  "engines": {
    "node": ">=20.18.1"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "{{author}}",
  "license": "MIT"
}
